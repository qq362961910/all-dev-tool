	"FLOORNUMBER" VARCHAR2(20) DEFAULT '',
	"ROOMID" NUMBER(20,0) DEFAULT 0,
	"BUILDINGNAME" VARCHAR2(50) DEFAULT '',
	"UNITNAME" VARCHAR2(50) DEFAULT '',
	"FIRM" VARCHAR2(50) DEFAULT '',
	"PLANTER" VARCHAR2(20) DEFAULT '',
	"AGELIMIT" NUMBER(3,0) DEFAULT 0,
	"DEVICEBIGTYPEID" NUMBER(20,0) DEFAULT 0,
	"DEVICEBIGTYPENAME" VARCHAR2(30) DEFAULT '',
	"ALARMCOUNT" NUMBER(10,0) DEFAULT 0,
	"MALFUNCTIONCOUNT" NUMBER(10,0) DEFAULT 0,
	"PRODUCTDATE" DATE,
	"MAINTENANCEUNIT" VARCHAR2(30) DEFAULT '',
	"MAINTENANCEUSER" VARCHAR2(20) DEFAULT '',
	"MAINTENANCEPHONE" VARCHAR2(20) DEFAULT '',
	"TRANSMISSIONID" NUMBER(22,0) DEFAULT 0 NOT NULL ENABLE,
	 PRIMARY KEY ("ID") ENABLE,
	 CHECK ("ID" IS NOT NULL) ENABLE,
	 UNIQUE ("MAC", "CONTROLLERID", "TRANSMISSIONID") ENABLE
   ) ;

