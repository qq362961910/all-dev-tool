
CREATE TABLE "ADDRESSBOOK"
   (	"ID" NUMBER(20,0) DEFAULT 0 NOT NULL ENABLE,
	"USERID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"LINKMAN" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"CELLPHONE" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"EMAIL" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"GROUPS" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"UNITID" NUMBER(20,0) DEFAULT 0 NOT NULL ENABLE,
	 CONSTRAINT "ADDRESSBOOK_ID" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "ADDRESSBOOK_GROUP"
   (	"ID" NUMBER(20,0) DEFAULT 0 NOT NULL ENABLE,
	"USERID" NUMBER(20,0) DEFAULT 0 NOT NULL ENABLE,
	"GROUPNAME" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	 CONSTRAINT "ADDRESSBOOK_GROUP_ID" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "ALARM_IMG"
