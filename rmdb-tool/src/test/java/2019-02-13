	"STATUS" NUMBER DEFAULT 1 NOT NULL ENABLE,
	"UNITID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"IDCODE" VARCHAR2(32) DEFAULT '',
	"CREATETIME" TIMESTAMP (6),
	"REVIEW" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"REVIEWCONTENT" VARCHAR2(20) DEFAULT NULL,
	"REVIEWID" NUMBER DEFAULT NULL,
	"PINYIN" VARCHAR2(20) DEFAULT '',
	"HEADIMGURL" VARCHAR2(200) DEFAULT '',
	"REVIEWNAME" VARCHAR2(50) DEFAULT '',
	"REVIEWTIME" DATE,
	"MESSAGE" NUMBER(2,0) DEFAULT 1,
	"UNIONID" VARCHAR2(64) DEFAULT '',
	"OPENID" VARCHAR2(64) DEFAULT '',
	"ORBIT" NUMBER(2,0) DEFAULT 0,
	 CONSTRAINT "XF_USER_PK" PRIMARY KEY ("ID") ENABLE,
	 UNIQUE ("USERNAME") ENABLE,
	 UNIQUE ("CELLPHONE") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_USER_APPLICATION"
   (	"ID" NUMBER(22,0) NOT NULL ENABLE,
	"USERID" NUMBER(22,0) DEFAULT 0 NOT NULL ENABLE,
	"APPLICATIONID" NUMBER(22,0) DEFAULT 0 NOT NULL ENABLE,
	"STATUS" NUMBER(2,0) DEFAULT 1 NOT NULL ENABLE,
	 PRIMARY KEY ("ID") ENABLE,
	 UNIQUE ("USERID", "APPLICATIONID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_USER_INFO"
   (	"ID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"USERID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"SEX" NUMBER DEFAULT 1,
	"BIRTHDAY" DATE DEFAULT sysdate,
	"REALNAME" VARCHAR2(100) DEFAULT '',
	"IDENTIFY" VARCHAR2(100) DEFAULT '',
	"STATUS" NUMBER DEFAULT 1,
	"ADDRESS" VARCHAR2(100) DEFAULT '',
	"POSITION" VARCHAR2(50) DEFAULT '',
	 CONSTRAINT "XF_USER_INFO_PK" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_USER_ROLE"
   (	"ID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"USERID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"ROLEID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	 CONSTRAINT "XF_USER_ROLE_PK" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_USER_SOURCE"
   (	"ID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"USERID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"SOURCEID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	 CONSTRAINT "XF_USER_SOURCE_PK" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_WORKER"
   (	"ID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"NAME" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"PHONE" VARCHAR2(100) DEFAULT '' NOT NULL ENABLE,
	"UNITID" NUMBER DEFAULT 0 NOT NULL ENABLE,
	"STATUS" NUMBER DEFAULT 1 NOT NULL ENABLE,
	"CREATETIME" DATE NOT NULL ENABLE,
	 CONSTRAINT "XF_WORKER_PK" PRIMARY KEY ("ID") ENABLE
   ) ;

---------------------------------------------------------------------

  CREATE TABLE "XF_WORK_PUNCH"
   (	"ID" NUMBER(22,0) NOT NULL ENABLE,
	"LOGINUSERID" NUMBER(22,0),
	"LOGINUSERPHONE" VARCHAR2(50),
	"LOGINUSERNICKNAME" VARCHAR2(50),
	"LOGINROLEID" NUMBER(22,0),
	"LOGINROLENAME" VARCHAR2(50),
	"PUNCHUSERID" NUMBER(22,0),
	"PUNCHUSERPHONE" VARCHAR2(50),
	"PUNCHUSERNICKNAME" VARCHAR2(50),
	"PUNCHROLEID" NUMBER(22,0),
	"PUNCHROLENAME" VARCHAR2(50),
	"PUNCHTIME" TIMESTAMP (6),
	"STATUS" NUMBER(2,0),
	"LOGINUNITID" NUMBER(22,0),
	"LOGINUNITNAME" VARCHAR2(100),
	 PRIMARY KEY ("ID") ENABLE
   ) ;

